package database;

import java.sql.*;
import java.util.*;

public class CollectionInfo {
	private int ownerID;
    private List<UserInfo> members;
    private String name;
    private int id;
    
    CollectionInfo(ResultSet rs) throws SQLException {
        ownerID = rs.getInt("user_id");
        name = rs.getString("name");
        id = rs.getInt("collection_id");
    }
}
